   "source": [
    "def children_cat(row):\n",
    "    if row['children'] == 0:\n",
    "        return '0'\n",
    "    elif row['children'] == 1:\n",
    "        return '1'\n",
    "    elif row['children'] == 2:\n",
    "        return '2'\n",
    "    else:\n",
    "        return '3' # Создаём новый столбец и категоризируем по количеству детей"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [],
   "source": [
